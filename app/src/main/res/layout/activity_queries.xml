    <ListView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:entries="@array/LIST"/>

</androidx.appcompat.widget.LinearLayoutCompat>